<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.1//EN" "http://struts.apache.org/dtds/struts-2.1.dtd">
<struts>
	<package name="account" namespace="/login" extends="struts-default">
		<action name="account_*" class="AccountAction" method="{1}">
			<result name="success">/web_backstage/dashboard.jsp</result>
			<result name="error">/web_login/login.jsp</result>
		</action>
	</package>
	<package name="ui" namespace="/ui" extends="struts-default">
		<action name="indexAction_*" class="IndexAction" method="{1}">
			
		</action>
	</package>
</struts>    
